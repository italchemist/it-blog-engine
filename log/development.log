  [1m[35mSQL (0.5ms)[0m  INSERT INTO "articles" ("content", "date", "permalink", "title") VALUES (?, ?, ?, ?)  [["content", "<p>Доброго <em>DateTime.Now()</em>!</p>\n\n<p>Приветствую вас в лаборатории <em>IT</em> алхимика. Здесь я буду рассказывать об всяких интересных <em>IT</em> штуках, как то: программирование, инструменты разработчика, разработка игр и прочее.</p>\n\n<p>Добро пожаловать!</p>"], ["date", Tue, 23 Oct 2012], ["permalink", "hello-world"], ["title", "Hello World!"]]
